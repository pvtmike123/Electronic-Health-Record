<div id="page-wrapper">

  <div class="container-fluid">

    <!-- Page Heading -->
    <div class="row">
      <div class="col-lg-12">
        <h1 class="page-header">
          Calendar
          <small>Month</small>
        </h1>
        <ol class="breadcrumb">
          <li class="active">
            <i class="fa fa-dashboard"></i>
            Home
          </li>
        </ol>
      </div>
    </div>
    <!-- /.row -->

    <%= month_calendar events: @meetings do |date, meetings| %>
    <%= date.day %>

    <% meetings.each do |meeting| %>
      <div class="meetings">
        <a><%= link_to meeting.name, meeting %></a>
      </div>
    <% end %>
  <% end %>

</div>

</div
